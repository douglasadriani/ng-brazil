{
  "name": "ng-brazil",
  "version": "1.4.7",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run copy && ng build packages",
    "build:fullapp": "npm run build:app && rimraf src/ng-brazil",
    "build:lib": "rimraf dist && ng-packagr -p ng-brazil/package.json",
    "build:sass": "npm rebuild node-sass",
    "publish-npm": "npm run build:lib && cd dist/ng-brazil && npm publish",
    "test": "npm run build:lib && npm run build:fullapp && npm run e2e",
    "copy": "mkdir -p src/ng-brazil/ && cp -R dist/ng-brazil/* src/ng-brazil",
    "testdemo": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4205",
    "kill": "kill $(lsof -t -i:4205)"
  },
  "dependencies": {
    "@angular/common": "9.0.0-rc.14",
    "@angular/compiler": "9.0.0-rc.14",
    "@angular/core": "9.0.0-rc.14",
    "@angular/forms": "9.0.0-rc.14",
    "@angular/http": ">8.0.0",
    "@angular/platform-browser": "9.0.0-rc.14",
    "@angular/platform-browser-dynamic": "9.0.0-rc.14",
    "@angular/router": ">=9.0.0",
    "angular2-text-mask": "^8.0.4",
    "core-js": "^2.4.1",
    "js-brasil": "^2.1.0",
    "rxjs": "^6.5.3",
    "text-mask-addons": "^3.7.2",
    "tslib": "^1.10.0",
    "webpack": "~4.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/schematics": "^0.4.0",
    "@angular/cli": "9.0.1",
    "@angular/compiler-cli": "9.0.1",
    "@angular/language-service": "9.0.1",
    "node-sass": "^4.12.0",
    "@types/jasmine": "~2.8.0",
    "@types/node": "~9.4.0",
    "codelyzer": "^5.0.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.4.5",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.1",
    "ts-node": "~5.0.0",
    "tsickle": "^0.27.1",
    "tslint": "~5.9.0",
    "typescript": "3.1.1"
  }
}
