{
  "name": "ng-brazil",
  "version": "1.4.7",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run copy && ng build packages",
    "build:fullapp": "npm run build:app && rimraf src/ng-brazil",
    "build:lib": "rimraf dist && ng-packagr -p ng-brazil/package.json",
    "build:sass": "npm rebuild node-sass",
    "publish-npm": "npm run build:lib && cd dist/ng-brazil && npm publish",
    "test": "npm run build:lib && npm run build:fullapp && npm run e2e",
    "copy": "mkdir -p src/ng-brazil/ && cp -R dist/ng-brazil/* src/ng-brazil",
    "testdemo": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4205",
    "kill": "kill $(lsof -t -i:4205)"
  },
  "dependencies": {
    "@angular/common": "9.0.1",
    "@angular/compiler": "9.0.1",
    "@angular/core": "9.0.1",
    "@angular/forms": "9.0.1",
    "@angular/http": "7.2.16",
    "@angular/platform-browser": "9.0.1",
    "@angular/platform-browser-dynamic": "9.0.1",
    "@angular/router": "9.0.1",
    "angular2-text-mask": "^9.0.0",
    "core-js": "^3.6.4",
    "js-brasil": "^2.1.0",
    "rxjs": "^6.5.3",
    "text-mask-addons": "^3.8.0",
    "tslib": "^1.10.0",
    "webpack": "~4.41.6",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular-devkit/schematics": "^9.0.2",
    "@angular/cli": "9.0.2",
    "@angular/compiler-cli": "9.0.1",
    "@angular/language-service": "9.0.1",
    "node-sass": "^4.13.1",
    "@types/jasmine": "~3.5.4",
    "@types/node": "~13.7.1",
    "codelyzer": "^5.0.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.0",
    "protractor": "^5.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "~8.6.2",
    "tsickle": "^0.38.0",
    "tslint": "~6.0.0",
    "typescript": "3.7.5"
  }
}
